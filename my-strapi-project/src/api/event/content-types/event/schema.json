{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "description": {
      "type": "blocks"
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "politics",
        "crypto",
        "computer science",
        "geopolitics",
        "trump",
        "nbaplayoffs"
      ]
    },
    "tags": {
      "type": "enumeration",
      "enum": [
        "Politics",
        "World Affairs",
        "Defense & Military"
      ]
    },
    "state": {
      "type": "boolean"
    },
    "creationDate": {
      "type": "datetime"
    },
    "startTime": {
      "type": "datetime"
    },
    "endTime": {
      "type": "datetime"
    },
    "resolutionTime": {
      "type": "datetime"
    },
    "result": {
      "type": "string"
    },
    "yesCount": {
      "type": "biginteger"
    },
    "noCount": {
      "type": "biginteger"
    },
    "totalVolume": {
      "type": "biginteger"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "currentYesProbability": {
      "type": "decimal"
    },
    "currentNoProbability": {
      "type": "decimal"
    },
    "creatorId": {
      "type": "string"
    },
    "resolutionSource": {
      "type": "text"
    }
  }
}
